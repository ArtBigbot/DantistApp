      <PasswordField fx:id="password" layoutX="215.0" layoutY="209.0" promptText="Пароль" />
      <Button fx:id="singIn" layoutX="396.0" layoutY="243.0" mnemonicParsing="false" text="Войти" />
      <Button fx:id="register" layoutX="447.0" layoutY="14.0" mnemonicParsing="false" text="Зарегистрироваться" />
      <Label fx:id="logInAndPassNotCorrect" layoutX="396.0" layoutY="200.0" prefHeight="17.0" prefWidth="250.0" textFill="#f20712" />
      <ComboBox fx:id="combobox" layoutX="215.0" layoutY="243.0" prefWidth="150.0" />
   </children>
</AnchorPane>
